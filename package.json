{
  "name": "haste",
  "version": "0.2.0",
  "private": true,
  "description": "Private Pastebin Server",
  "keywords": [
    "paste",
    "pastebin"
  ],
  "author": {
    "name": "John Crepezzi",
    "email": "john.crepezzi@gmail.com",
    "url": "http://seejohncode.com/"
  },
  "main": "haste",
  "dependencies": {
    "connect": "3.6.5",
    "connect-ensure-login": "^0.1.1",
    "connect-query": "^1.0.0",
    "connect-ratelimit": "0.0.7",
    "connect-redirection": "0.0.1",
    "connect-route": "0.1.5",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "st": "^1.2.2",
    "uglify-js": "1.3.5",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "busboy": "0.2.14",
    "mocha": "*",
    "should": "*"
  },
  "bundledDependencies": [],
  "bin": {
    "haste-server": "./server.js"
  },
  "files": [
    "server.js",
    "lib",
    "static"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon -V --ignore 'data/*' --ignore 'Profile' --ignore 'static/application.min.js'  server.js < /dev/null",
    "test": "mocha -r should spec/*"
  }
}
